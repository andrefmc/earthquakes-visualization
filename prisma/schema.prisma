// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Earthquake {
  id        Int     @id @default(autoincrement())
  time      String
  latitude  Float
  longitude Float
  mag       String?
  place     String?
}
